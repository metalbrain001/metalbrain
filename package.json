{
  "name": "metalbrain",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "nodemon --watch",
    "tsc-watch": "tsc -b --watch",
    "start": "concurrently \"npm run tsc-watch\" \"npm run server\" \"npm run dev\"",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "postinstall": "npm list --depth=0",
    "migration:create": "npx sequelize-cli migration:create --name",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:rollback": "evn-cmd npx sequelize-cli db:migrate:undo",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "jest"
  },
